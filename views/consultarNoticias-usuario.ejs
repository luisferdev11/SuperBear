<!DOCTYPE html>
<html lang="en">

<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport"
        content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="default">
    <meta name="format-detection" content="telephone=no">

    <meta name="theme-color" content="#ffffff">
    <title>Noticias | SuperBear</title>
    <!-- favicon -->
    <link rel="icon" type="image/png" href="/assets/svg/logo.svg" sizes="32x32">

    <!-- CSS Libraries-->
    <!-- bootstrap v4.6.0 -->
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css">
    <!--
        theiconof v3.0
        https://www.theiconof.com/search
     -->
    <link rel="stylesheet" href="/assets/css/icons.css">
    <!-- Remix Icon -->
    <link rel="stylesheet" href="/assets/css/remixicon.css">
    <!-- Swiper 6.4.11 -->
    <link rel="stylesheet" href="/assets/css/swiper-bundle.min.css">
    <!-- Owl Carousel v2.3.4 -->
    <link rel="stylesheet" href="/assets/css/owl.carousel.min.css">
    <!-- Custom styles for this template -->
    <link rel="stylesheet" href="/assets/css/main.css">
    <!-- normalize.css v8.0.1 -->
    <link rel="stylesheet" href="/assets/css/normalize.css">
</head>


<body>

    <!-- Start em_loading -->
    <section class="em_loading" id="loaderPage">
        <div class="spinner_flash"></div>
    </section>
    <!-- End. em_loading -->
    <header class="main_haeder header-sticky multi_item">
        <!-- <div class="em_side_right">
            <a class="rounded-circle d-flex align-items-center text-decoration-none" href="app-pages.html">
                <i class="tio-chevron_left size-24 color-text"></i>
                <span class="color-text size-14">Back</span>
            </a>
        </div> -->
        <div class="title_page">
            <span class="page_name">
                Noticias
            </span>
        </div>
        <!-- <div class="em_side_right">
            <button type="button" class="btn btn_meunSearch" id="saerch-On-header">
                <i class="ri-search-2-line"></i>
            </button>
        </div> -->
    </header>
    <div id="wrapper">
        <div id="content">
            
            <!-- Start page_news -->
            <section class="page_news pb-5">
                <!--Mini barra de navegacion, por si se aÃ±ade lo de twitter 
                    
                    <div class="emBlock__border padding-l-20 bg-snow">
                    <ul class="nav navListProducts">
                        <li class="nav-item">
                            <a class="nav-link active" href="#">New</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Marketing</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Design</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Software</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Photography</a>
                        </li>
                    </ul>
                </div> -->
                <% noticias.forEach(element =>{ %>
                <ul class="itemList__news">
                    <li class="items-nav">
                        <a class="btn">
                            <div class="media align-items-center">
                                <embed class="img_news" src="https://placewaifu.com/image/?<%= element.idDNoticias %>" alt="">
                                <div class="media-body">
                                    <div class="txt">
                                        <h5 class="m-0" style="height: fit-content;"><%= element.tit_not %></h5>
                                        <span>
                                            <%= element.cont_not %>
                                        </span>
                                        <div class="info_bottom">
                                            <div class="time">
                                                <div class="icon">
                                                    <svg id="Iconly_Curved_Time_Square"
                                                        data-name="Iconly/Curved/Time Square"
                                                        xmlns="http://www.w3.org/2000/svg" width="15" height="15"
                                                        viewBox="0 0 15 15">
                                                        <g id="Time_Square" data-name="Time Square"
                                                            transform="translate(1.719 1.719)">
                                                            <path id="Stroke_1" data-name="Stroke 1"
                                                                d="M0,5.781c0,4.336,1.446,5.781,5.781,5.781s5.781-1.446,5.781-5.781S10.117,0,5.781,0,0,1.446,0,5.781Z"
                                                                fill="none" stroke="#cbcdd8" stroke-linecap="round"
                                                                stroke-linejoin="round" stroke-miterlimit="10"
                                                                stroke-width="1.5" />
                                                            <path id="Stroke_3" data-name="Stroke 3"
                                                                d="M2.119,3.99,0,2.726V0"
                                                                transform="translate(5.781 3.053)" fill="none"
                                                                stroke="#cbcdd8" stroke-linecap="round"
                                                                stroke-linejoin="round" stroke-miterlimit="10"
                                                                stroke-width="1.5" />
                                                        </g>
                                                    </svg>
                                                </div>
                                                <span>
                                                    <% let mes = parseInt(element.fec_not.getMonth()) + 1 %>
                                                    <%= element.fec_not.getDate() + '-' + mes + '-' + element.fec_not.getFullYear() %>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </li>
                </ul>
                <% }) %> 
            </section>
            <!-- End. page_news -->

            <!-- spinner_loading -->
            <!-- <div class="spinner_loading">
                <div class="bounce1"></div>
                <div class="bounce2"></div>
                <div class="bounce3"></div>
            </div> -->
            <!-- End. spinner_loading -->

        </div>
    </div>
    <footer class="em_main_footer with__text p-0">
        <div id="footerNuevo" class="em_body_navigation -active-links">
            <div class="col-4 item_link">
                <a href="/datos-perfil" class="btn btn_navLink">
                    <div class="icon_current">
                        <img src="/assets/svg/user.svg" id="Iconly_Two-tone_Discovery" data-name="Iconly/Two-tone/Discovery" width="24" height="24" viewBox="0 0 24 24">
                    </div>
                    <div class="icon_active">
                        <img src="/assets/svg/user-active.svg" id="Iconly_Bulk_Discovery" data-name="Iconly/Bulk/Discovery" width="24" height="24" viewBox="0 0 24 24">

                    </div>
                    <div class="txt__tile">Perfil</div>
                </a>
            </div>
            <div class="col-4 item_link">
                <a href="/Misgrupos" class="btn btn_navLink">
                    <div class="icon_current">
                        <img src="/assets/svg/grupos.svg" id="Iconly_Two-tone_Folder" data-name="Iconly/Two-tone/Folder" width="24" height="24" viewBox="0 0 24 24">
                    </div>
                    <div class="icon_active">
                        <img src="/assets/svg/grupos-active.svg" id="Iconly_Bulk_Folder" data-name="Iconly/Bulk/Folder" width="24" height="24" viewBox="0 0 24 24">
                    </div>
                    <div class="txt__tile">Grupos</div>
                </a>
            </div>
            <div class="col-4 item_link">
                <a href="/consultarNoticias-usuario" class="btn btn_navLink active">
                    <div class="icon_current">
                        <img src="/assets/svg/news.svg" id="Iconly_Two-tone_Location" data-name="Iconly/Two-tone/Location" width="24" height="24" viewBox="0 0 24 24">
                    </div>
                    <div class="icon_active">
                        <img src="/assets/svg/news-active.svg" id="Iconly_Bulk_Location" data-name="Iconly/Bulk/Location" width="24" height="24" viewBox="0 0 24 24">

                    </div>
                    <div class="txt__tile">Noticias</div>
                </a>
            </div>
        </div>
    </footer>

    <!-- jquery -->
    <script src="/assets/js/jquery-3.6.0.js"></script>
    <!-- popper.min.js 1.16.1 -->
    <script src="/assets/js/popper.min.js"></script>
    <!-- bootstrap.js v4.6.0 -->
