    <!-- Custom styles for this template -->
    <link rel="stylesheet" href="/assets/css/main.css">
    <!-- normalize.css v8.0.1 -->
    <link rel="stylesheet" href="/assets/css/normalize.css">
</head>

<body>
    <!-- jquery -->
    <script src="/assets/js/jquery-3.6.0.js"></script>
    <!-- popper.min.js 1.16.1 -->
    <script src="/assets/js/popper.min.js"></script>
    <!-- bootstrap.js v4.6.0 -->
    <script src="/assets/js/bootstrap.min.js"></script>

    <!-- Owl Carousel v2.3.4 -->
    <script src="/assets/js/vendor/owl.carousel.min.js"></script>
    <!-- Swiper 6.4.11 -->
    <script src="/assets/js/vendor/swiper-bundle.min.js"></script>
    <!-- sharer 0.4.0 -->
    <script src="/assets/js/vendor/sharer.js"></script>
    <!-- short-and-sweet v1.0.2 - Accessible character counter for input elements -->
    <script src="/assets/js/vendor/short-and-sweet.min.js"></script>
    <!-- jquery knob -->
    <script src="/assets/js/vendor/jquery.knob.min.js"></script>
    <!-- main.js -->
    <script src="/assets/js/main.js" defer></script>
    <!-- PWA app service registration and works js -->
    <script src="/assets/js/pwa-services.js"></script>


    <br>
    <br>
    <br>
    <br>

    <header class="main_haeder header-sticky multi_item">
        <div class="em_side_right">
            <a class="rounded-circle d-flex align-items-center text-decoration-none" href="/misgrupos">
                <i class="tio-chevron_left size-24 color-text"></i>
                <span class="color-text size-14">Regresar</span>
            </a>
        </div>
        <div class="title_page">
            <span class="page_name">
                Miembros</span>
        </div>
        <div class="em_side_left">
            <a class="rounded-circle d-flex align-items-center text-decoration-none" href="/abandonargrupo/<%=grupo%>/<%=id_usuario%>">
                    Abandonar Grupo
            </a>
        </div>
    </header>
    <section class="em_loading" id="loaderPage">
        <div class="spinner_flash"></div>
    </section>

    <% for (let i=0; i < miembros.length; i++) { %>
        <section class="em__pkLink">
    
        <ul class="nav__list mb-0 emBlock__border bg-white py-4">
            <div class="group_collection">
        <li>
            <a  class="item-link">
                <div class="group">
                    <div class="icon bg-yellow">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 101 101"><path d="M50.4 54.5c10.1 0 18.2-8.2 18.2-18.2S60.5 18 50.4 18s-18.2 8.2-18.2 18.2 8.1 18.3 18.2 18.3zm0-31.7c7.4 0 13.4 6 13.4 13.4s-6 13.4-13.4 13.4S37 43.7 37 36.3s6-13.5 13.4-13.5zM18.8 83h63.4c1.3 0 2.4-1.1 2.4-2.4 0-12.6-10.3-22.9-22.9-22.9H39.3c-12.6 0-22.9 10.3-22.9 22.9 0 1.3 1.1 2.4 2.4 2.4zm20.5-20.5h22.4c9.2 0 16.7 6.8 17.9 15.7H21.4c1.2-8.9 8.7-15.7 17.9-15.7z"/></svg>
                    </div>
                    <span class="path__name"><%=miembros[i]%></span>
                </div>
                <div class="group">
                    <span class="short__name"><% if(privilegio[i]==1){ %>
                        <%="Admin"%>
                            <% }else{ %>
                                <%="Miembro"%>
                                    <% } %>
                </b></span>
                    
                </div>
            </a>
        </li></div></ul></section>
        <%}%>
    
    </div>
    <br>
    <script src="../js/detect-browser.js"></script>
    <script src="../js/barraNavegacion.js"></script>
</body>

</html>